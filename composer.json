{
  "name": "expresspayments/expresspayments-php",
  "description": "ExpressPayments PHP Library",
  "keywords": [
    "epayments",
    "expresspayments",
    "expressplatby",
    "payment processing",
    "api"
  ],
  "version": "1.1.0",
  "homepage": "https://epayments.network/",
  "license": "MIT",
  "authors": [
    {
      "name": "ExpressPayments and contributors",
      "homepage": "https://github.com/expresspayments/expresspayments-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7 || ^9.0",
    "friendsofphp/php-cs-fixer": "3.5.0",
    "phpstan/phpstan": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "ExpressPayments\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ExpressPayments\\": [
        "tests/",
        "tests/ExpressPayments/"
      ]
    }
  }
}
